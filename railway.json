{
    "build": {
        "builder": "Dockerfile"
    },
    "deploy": {
        "startCommand": "chroma --host 0.0.0.0 --port 8000",
        "restartPolicyType": "ON_FAILURE"
    },
    "services": [
        {
            "name": "chroma",
            "port": 8000,
            "protocol": "http"
        }
    ]
}